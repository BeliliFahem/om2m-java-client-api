<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="MyApp" packages="">
  <Appenders>
    <File name="mainFile" fileName="logs/om2mobj.log">
      <PatternLayout>
        <Pattern>%d %p %C{1.}:%L [%t] : %m%n</Pattern>
      </PatternLayout>
    </File>
  </Appenders>
  <Loggers>
    <Root level="debug">
      <AppenderRef ref="mainFile"/>
    </Root>
  </Loggers>
</Configuration>